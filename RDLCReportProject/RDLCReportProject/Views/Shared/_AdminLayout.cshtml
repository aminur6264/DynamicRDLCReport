<!doctype html>
<html class="fixed">
<head>

    <!-- Basic -->
    <meta charset="UTF-8">

    <title>Dashboard | </title>
    @*<meta name="keywords" content="HTML5 Admin Template" />*@
    @*<meta name="description" content="Porto Admin - Responsive HTML5 Template">*@
    @*<meta name="author" content="okler.net">*@

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link href="~/Theme/Admin/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Theme/Admin/css/bootstrap-fileupload.min.css" rel="stylesheet" />

    <link href="~/Theme/Admin/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Theme/Admin/css/magnific-popup.css" rel="stylesheet" />
    <link href="~/Theme/Admin/css/datepicker3.css" rel="stylesheet" />
    <link href="~/Theme/Notifications/css/mk-notifications.min.css" rel="stylesheet" />

    <!-- Specific Page Vendor CSS -->
    <link href="~/Theme/Admin/css/jquery-ui-1.10.4.custom.css" rel="stylesheet" />
    <link href="~/Theme/Admin/css/bootstrap-multiselect.css" rel="stylesheet" />
    <link href="~/Theme/Admin/css/morris.css" rel="stylesheet" />

    <!-- Theme CSS -->
    <link href="~/Theme/Admin/css/theme.css" rel="stylesheet" />

    <!-- Skin CSS -->
    <link href="~/Theme/Admin/css/default.css" rel="stylesheet" />

    <!-- Theme Custom CSS -->
    <link href="~/Theme/Admin/css/theme-custom.css" rel="stylesheet" />
    <link href="~/Theme/Admin/css/bootstrap-custom.css" rel="stylesheet" />

    <!-- Head Libs -->
    <script src="~/Theme/Admin/js/modernizr.js"></script>
</head>
<body>
    <section class="body">

        <!-- start: header -->
        @Html.Partial("PartialViews/_TopBarMenu")
        <!-- end: header -->

        <div class="inner-wrapper">
            <!-- start: sidebar -->
            @Html.Partial("PartialViews/_SideBarMenu")
            <!-- end: sidebar -->

            <section role="main" class="content-body">
                <!-- start: page -->
                @RenderBody()
                <!-- end: page -->
            </section>
        </div>

        <aside id="sidebar-right" class="sidebar-right">
            <div class="nano">
                <div class="nano-content">
                    <a href="#" class="mobile-close visible-xs">
                        Collapse <i class="fa fa-chevron-right"></i>
                    </a>

                    <div class="sidebar-right-wrapper">

                        <div class="sidebar-widget widget-calendar">
                            <h6>Upcoming Tasks</h6>
                            <div data-plugin-datepicker data-plugin-skin="dark"></div>

                            <ul>
                                <li>
                                    <time datetime="2014-04-19T00:00+00:00">04/19/2014</time>
                                    <span>Company Meeting</span>
                                </li>
                            </ul>
                        </div>

                        <div class="sidebar-widget widget-friends">
                            <h6>Friends</h6>
                            <ul>
                                <li class="status-online">
                                    <figure class="profile-picture">
                                        <img src="assets/images/!sample-user.jpg" alt="Joseph Doe" class="img-circle">
                                    </figure>
                                    <div class="profile-info">
                                        <span class="name">Joseph Doe Junior</span>
                                        <span class="title">Hey, how are you?</span>
                                    </div>
                                </li>
                                <li class="status-online">
                                    <figure class="profile-picture">
                                        <img src="assets/images/!sample-user.jpg" alt="Joseph Doe" class="img-circle">
                                    </figure>
                                    <div class="profile-info">
                                        <span class="name">Joseph Doe Junior</span>
                                        <span class="title">Hey, how are you?</span>
                                    </div>
                                </li>
                                <li class="status-offline">
                                    <figure class="profile-picture">
                                        <img src="assets/images/!sample-user.jpg" alt="Joseph Doe" class="img-circle">
                                    </figure>
                                    <div class="profile-info">
                                        <span class="name">Joseph Doe Junior</span>
                                        <span class="title">Hey, how are you?</span>
                                    </div>
                                </li>
                                <li class="status-offline">
                                    <figure class="profile-picture">
                                        <img src="assets/images/!sample-user.jpg" alt="Joseph Doe" class="img-circle">
                                    </figure>
                                    <div class="profile-info">
                                        <span class="name">Joseph Doe Junior</span>
                                        <span class="title">Hey, how are you?</span>
                                    </div>
                                </li>
                            </ul>
                        </div>

                    </div>
                </div>
            </div>
        </aside>
    </section>

    <!-- Vendor -->
    <script src="~/Theme/Admin/js/jquery.js"></script>
    <script src="~/Theme/Admin/js/jquery.browser.mobile.js"></script>
    <script src="~/Theme/Admin/js/bootstrap.min.js"></script>
    <script src="~/Theme/Admin/js/nanoscroller.js"></script>
    @*<script src="assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>*@
    <script src="~/Theme/Admin/js/magnific-popup.js"></script>
    <script src="~/Theme/Admin/js/jquery.placeholder.js"></script>

    <!-- Specific Page Vendor -->
    <script src="~/Theme/Admin/js/jquery-ui-1.10.4.custom.js"></script>
    <script src="~/Theme/Admin/js/jquery.ui.touch-punch.js"></script>
    <script src="~/Theme/Admin/js/jquery.appear.js"></script>
    <script src="~/Theme/Admin/js/bootstrap-multiselect.js"></script>
    <script src="~/Theme/Admin/js/bootstrap-fileupload.min.js"></script>
    @*<script src="assets/vendor/jquery-easypiechart/jquery.easypiechart.js"></script>*@
    <script src="~/Theme/Admin/js/jquery.flot.min.js"></script>
    <script src="~/Theme/Admin/js/jquery.flot.tooltip.js"></script>
    @*<script src="assets/vendor/flot/jquery.flot.pie.js"></script>*@
    <script src="~/Theme/Admin/js/jquery.flot.categories.js"></script>
    <script src="~/Theme/Admin/js/jquery.flot.resize.js"></script>
    <script src="~/Theme/Admin/js/jquery.sparkline.js"></script>
    <script src="~/Theme/Admin/js/raphael.js"></script>
    <script src="~/Theme/Admin/js/morris.min.js"></script>
    <script src="~/Theme/Admin/js/gauge.js"></script>
    <script src="~/Theme/Admin/js/snap.svg.js"></script>
    <script src="~/Theme/Admin/js/liquid.meter.js"></script>
    <link href="~/Theme/Admin/css/dropzon/basic.css" rel="stylesheet" />
    <link href="~/Theme/Admin/css/dropzon/dropzone.css" rel="stylesheet" />
    <script src="~/Theme/Notifications/js/mk-notifications.js"></script>
    @*<script src="assets/vendor/jqvmap/jquery.vmap.js"></script>*@
    @*<script src="assets/vendor/jqvmap/data/jquery.vmap.sampledata.js"></script>*@
    @*<script src="assets/vendor/jqvmap/maps/jquery.vmap.world.js"></script>*@
    @*<script src="assets/vendor/jqvmap/maps/continents/jquery.vmap.africa.js"></script>*@
    @*<script src="assets/vendor/jqvmap/maps/continents/jquery.vmap.asia.js"></script>*@
    @*<script src="assets/vendor/jqvmap/maps/continents/jquery.vmap.australia.js"></script>*@
    @*<script src="assets/vendor/jqvmap/maps/continents/jquery.vmap.europe.js"></script>*@
    @*<script src="assets/vendor/jqvmap/maps/continents/jquery.vmap.north-america.js"></script>*@
    @*<script src="assets/vendor/jqvmap/maps/continents/jquery.vmap.south-america.js"></script>*@

    <!-- Theme Base, Components and Settings -->
    <script src="~/Theme/Admin/js/theme/theme.js"></script>

    <!-- Theme Custom -->
    <script src="~/Theme/Admin/js/theme/theme.custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="~/Theme/Admin/js/theme/theme.init.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>